{
  "name": "jomon-ui",
  "version": "1.0.0",
  "scripts": {
    "dev": "vite",
    "serve": "vite preview",
    "build": "vue-tsc --noEmit && vite build",
    "build:watch": "vite build --watch",
    "lint": "eslint --fix --ext .js,.ts,.vue --cache .",
